{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "skipLibCheck": true,

    "moduleResolution": "Bundler",
    "allowArbitraryExtensions": true,

    "esModuleInterop": true,

    "forceConsistentCasingInFileNames": true,

    /* Type Checking */
    "strict": true,
    "jsx": "react-jsx"
  }
}
